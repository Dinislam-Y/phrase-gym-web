  "theory": {
    "category": "grammar",
    "title": "Much / Many / A Lot Of",
    "subtitle": "Слова для количества. Much — для воды и денег (нельзя посчитать), many — для книг и людей (можно посчитать), a lot of — для всего.",
    "construction": {
      "formula": "Much (uncountable) / Many (countable) / A lot of (any)",
      "rules": [
        {
          "word": "much",
          "description": "С неисчисляемыми (вода, время, деньги): How much water? Not much time."
        },
        {
          "word": "many",
          "description": "С исчисляемыми (книги, люди, дни): How many books? Many people came."
        },
        {
          "word": "a lot of",
          "description": "С любыми — в утверждениях: I have a lot of friends / a lot of time."
        },
        {
          "word": "a few / a little",
          "description": "A few + исчисляемое (a few books). A little + неисчисляемое (a little water)."
        }
      ]
    },
    "useCases": [
      {
        "icon": "explore",
        "color": "blue",
        "title": "Спрашиваем о количестве",
        "description": "How much (неисчисл.) / How many (исчисл.) — для вопросов.",
        "example": {
          "english": "How much water do you need?",
          "russian": "Сколько воды тебе нужно?"
        }
      },
      {
        "icon": "check_circle",
        "color": "green",
        "title": "Говорим «много»",
        "description": "A lot of — универсальное «много» для утверждений.",
        "example": {
          "english": "We have a lot of food.",
          "russian": "У нас много еды."
        }
      },
      {
        "icon": "star",
        "color": "orange",
        "title": "Немного / Несколько",
        "description": "A little (чуть-чуть) и a few (несколько) — для маленьких количеств.",
        "example": {
          "english": "I have a few friends here.",
          "russian": "У меня есть несколько друзей здесь."
        }
      }
    ],
    "markers": [
      "how much",
      "how many",
      "a lot of",
      "a few",
      "a little",
      "much",
      "many"
    ]
  },
