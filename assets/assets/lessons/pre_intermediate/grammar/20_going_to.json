  "theory": {
    "category": "grammar",
    "title": "Going To: Future Plans",
    "subtitle": "Когда вы уже решили, что собираетесь делать. Going to — для планов и намерений на будущее.",
    "construction": {
      "formula": "S + am/is/are + going to + V1",
      "rules": [
        {
          "word": "I am",
          "description": "I am going to travel. (Я собираюсь путешествовать.)"
        },
        {
          "word": "He / She / It is",
          "description": "She is going to study. (Она собирается учиться.)"
        },
        {
          "word": "You / We / They are",
          "description": "We are going to move. (Мы собираемся переехать.)"
        },
        {
          "word": "Отрицание (No)",
          "description": "I am not going to cook. She is not going to leave."
        }
      ]
    },
    "useCases": [
      {
        "icon": "schedule",
        "color": "blue",
        "title": "Планы на будущее",
        "description": "Говорим о том, что уже решили сделать.",
        "example": {
          "english": "I am going to travel.",
          "russian": "Я собираюсь путешествовать."
        }
      },
      {
        "icon": "star",
        "color": "purple",
        "title": "Намерения",
        "description": "Выражаем намерение что-то сделать.",
        "example": {
          "english": "She is going to learn English.",
          "russian": "Она собирается учить английский."
        }
      },
      {
        "icon": "visibility",
        "color": "orange",
        "title": "Очевидное будущее",
        "description": "Когда видно, что что-то вот-вот произойдёт.",
        "example": {
          "english": "It is going to rain.",
          "russian": "Сейчас пойдёт дождь."
        }
      }
    ],
    "markers": [
      "tomorrow",
      "next week",
      "next month",
      "soon",
      "tonight",
      "this weekend"
    ]
  },
