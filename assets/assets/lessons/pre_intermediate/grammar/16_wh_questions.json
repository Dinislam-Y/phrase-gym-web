  "theory": {
    "category": "grammar",
    "title": "Wh- Questions",
    "subtitle": "Вопросительные слова открывают двери к информации. С ними вы задаёте открытые вопросы — не просто «да/нет», а конкретные.",
    "construction": {
      "formula": "Wh- + do/does/is/are/was/were + S + V",
      "rules": [
        {
          "word": "What",
          "description": "Что? Какой? — What do you want? What is your name?"
        },
        {
          "word": "Where",
          "description": "Где? Куда? — Where do you live? Where is the shop?"
        },
        {
          "word": "When",
          "description": "Когда? — When do you work? When is the meeting?"
        },
        {
          "word": "Who",
          "description": "Кто? — Who is he? Who called you?"
        },
        {
          "word": "Why",
          "description": "Почему? — Why are you late? Why do you study?"
        },
        {
          "word": "How",
          "description": "Как? — How are you? How do you cook this?"
        }
      ]
    },
    "useCases": [
      {
        "icon": "explore",
        "color": "blue",
        "title": "Узнаём информацию",
        "description": "Задаём открытые вопросы, чтобы получить конкретный ответ.",
        "example": {
          "english": "Where do you work?",
          "russian": "Где ты работаешь?"
        }
      },
      {
        "icon": "groups",
        "color": "purple",
        "title": "Знакомство и общение",
        "description": "Спрашиваем имя, возраст, место жительства.",
        "example": {
          "english": "What is your name?",
          "russian": "Как тебя зовут?"
        }
      },
      {
        "icon": "schedule",
        "color": "orange",
        "title": "Время и расписание",
        "description": "Уточняем, когда что-то происходит.",
        "example": {
          "english": "When does the train leave?",
          "russian": "Когда уходит поезд?"
        }
      }
    ],
    "markers": [
      "what",
      "where",
      "when",
      "who",
      "why",
      "how"
    ]
  },
