  "theory": {
    "category": "grammar",
    "title": "Some / Any / No",
    "subtitle": "Три маленьких слова, которые показывают, есть что-то или нет. Some — в утверждениях, any — в вопросах и отрицаниях, no — когда ничего нет.",
    "construction": {
      "formula": "Some (+) / Any (? / -) / No (= not any)",
      "rules": [
        {
          "word": "some",
          "description": "В утверждениях (+): I have some milk. There are some books."
        },
        {
          "word": "any",
          "description": "В вопросах (?) и отрицаниях (-): Do you have any milk? I don't have any books."
        },
        {
          "word": "no",
          "description": "Полное отрицание без don't: I have no money. There is no water."
        },
        {
          "word": "-thing / -one / -body",
          "description": "something, anything, nothing; someone, anyone, nobody — по тем же правилам."
        }
      ]
    },
    "useCases": [
      {
        "icon": "check_circle",
        "color": "green",
        "title": "Говорим, что есть",
        "description": "Используем some в утвердительных предложениях.",
        "example": {
          "english": "I have some friends.",
          "russian": "У меня есть друзья."
        }
      },
      {
        "icon": "explore",
        "color": "blue",
        "title": "Спрашиваем",
        "description": "Используем any в вопросах.",
        "example": {
          "english": "Do you have any water?",
          "russian": "У тебя есть вода?"
        }
      },
      {
        "icon": "visibility",
        "color": "orange",
        "title": "Говорим, что нет",
        "description": "Используем no для полного отрицания.",
        "example": {
          "english": "I have no time.",
          "russian": "У меня нет времени."
        }
      }
    ],
    "markers": [
      "some",
      "any",
      "no",
      "something",
      "anything",
      "nothing",
      "someone",
      "anyone"
    ]
  },
