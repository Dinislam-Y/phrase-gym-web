  "theory": {
    "category": "grammar",
    "title": "Like / Love / Hate + -ing",
    "subtitle": "Как сказать, что вам нравится или не нравится делать. После like, love, hate, enjoy ставим глагол с -ing.",
    "construction": {
      "formula": "S + like/love/hate/enjoy + V-ing",
      "rules": [
        {
          "word": "like + -ing",
          "description": "Нравится: I like swimming. She likes reading."
        },
        {
          "word": "love + -ing",
          "description": "Обожаю: I love dancing. He loves cooking."
        },
        {
          "word": "hate + -ing",
          "description": "Ненавижу: I hate cleaning. She hates waiting."
        },
        {
          "word": "enjoy + -ing",
          "description": "Получаю удовольствие: I enjoy walking. We enjoy traveling."
        },
        {
          "word": "don't mind + -ing",
          "description": "Не возражаю: I don't mind waiting. He doesn't mind helping."
        }
      ]
    },
    "useCases": [
      {
        "icon": "favorite",
        "color": "green",
        "title": "Что нравится",
        "description": "Like и love + -ing для приятных занятий.",
        "example": {
          "english": "I love traveling.",
          "russian": "Я обожаю путешествовать."
        }
      },
      {
        "icon": "bolt",
        "color": "orange",
        "title": "Что не нравится",
        "description": "Hate + -ing для неприятных занятий.",
        "example": {
          "english": "I hate cleaning.",
          "russian": "Я ненавижу уборку."
        }
      },
      {
        "icon": "check_circle",
        "color": "blue",
        "title": "Нейтральное отношение",
        "description": "Don't mind + -ing — когда не против.",
        "example": {
          "english": "I don't mind waiting.",
          "russian": "Я не против подождать."
        }
      }
    ],
    "markers": [
      "like",
      "love",
      "hate",
      "enjoy",
      "don't mind"
    ]
  },
