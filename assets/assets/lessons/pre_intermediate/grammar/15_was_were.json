  "theory": {
    "category": "grammar",
    "title": "Past Simple: Was / Were",
    "subtitle": "Глагол «быть» в прошедшем времени. Без него нельзя описать, что было вчера, на прошлой неделе или год назад.",
    "construction": {
      "formula": "Кто + was/were + ...",
      "rules": [
        {
          "word": "I / He / She / It",
          "description": "Используем WAS: I was tired. She was happy."
        },
        {
          "word": "You / We / They",
          "description": "Используем WERE: You were right. They were at home."
        },
        {
          "word": "Отрицание (No)",
          "description": "was not (wasn't), were not (weren't): I wasn't late."
        },
        {
          "word": "Вопрос (?)",
          "description": "Was/Were + подлежащее: Was she happy? Were they home?"
        }
      ]
    },
    "useCases": [
      {
        "icon": "schedule",
        "color": "blue",
        "title": "Состояние в прошлом",
        "description": "Описываем, каким кто-то или что-то было раньше.",
        "example": {
          "english": "I was tired yesterday.",
          "russian": "Я устал вчера."
        }
      },
      {
        "icon": "explore",
        "color": "purple",
        "title": "Место в прошлом",
        "description": "Говорим, где кто-то находился.",
        "example": {
          "english": "We were at home.",
          "russian": "Мы были дома."
        }
      },
      {
        "icon": "visibility",
        "color": "orange",
        "title": "Факты о прошлом",
        "description": "Описываем факты и ситуации, которые уже прошли.",
        "example": {
          "english": "It was cold last night.",
          "russian": "Вчера вечером было холодно."
        }
      }
    ],
    "markers": [
      "yesterday",
      "last week",
      "last month",
      "ago",
      "last night",
      "in 2020"
    ]
  },
