  "theoryQuiz": [
    {
      "id": "tq_01",
      "type": "trueFalse",
      "question": "Subject pronouns (I, he, she...) используются, когда местоимение выполняет действие.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "Subject pronouns — подлежащее, тот кто выполняет действие: He reads. She sings."
    },
    {
      "id": "tq_02",
      "type": "multipleChoice",
      "question": "Какое Object pronoun соответствует \"he\"?",
      "options": [
        "his",
        "he",
        "him",
        "her"
      ],
      "correctIndex": 2,
      "explanation": "He (subject) → him (object): I see him."
    },
    {
      "id": "tq_03",
      "type": "multipleChoice",
      "question": "Выберите правильный вариант: \"I gave ___ a present.\" (ей)",
      "options": [
        "she",
        "her",
        "hers",
        "his"
      ],
      "correctIndex": 1,
      "explanation": "Her — object pronoun от she. Используется когда кто-то получает действие."
    },
    {
      "id": "tq_04",
      "type": "trueFalse",
      "question": "\"My\" — это possessive adjective (притяжательное прилагательное).",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "My, your, his, her, its, our, their — possessive adjectives, отвечают на вопрос «Чей?»."
    },
    {
      "id": "tq_05",
      "type": "multipleChoice",
      "question": "Выберите правильный вариант: \"___ is a good student.\" (Она)",
      "options": [
        "Her",
        "Hers",
        "She",
        "Him"
      ],
      "correctIndex": 2,
      "explanation": "She — subject pronoun, используется как подлежащее: She is a good student."
    },
    {
      "id": "tq_06",
      "type": "trueFalse",
      "question": "\"Me like ice cream.\" — предложение составлено правильно.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 1,
      "explanation": "Me — object pronoun. Для подлежащего нужно I: I like ice cream."
    },
    {
      "id": "tq_07",
      "type": "multipleChoice",
      "question": "Какое possessive adjective соответствует \"they\"?",
      "options": [
        "them",
        "they",
        "their",
        "theirs"
      ],
      "correctIndex": 2,
      "explanation": "They → their (притяжательное): their house, their children."
    },
    {
      "id": "tq_08",
      "type": "multipleChoice",
      "question": "Выберите правильное предложение:",
      "options": [
        "Him is my friend.",
        "I know they.",
        "Please call us.",
        "Her likes cats."
      ],
      "correctIndex": 2,
      "explanation": "Call us — us это object pronoun. Him/Her — object (не subject), I know them (не they)."
    },
    {
      "id": "tq_09",
      "type": "trueFalse",
      "question": "\"This is our car.\" — \"our\" здесь является possessive adjective.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "Our — possessive adjective от we. Отвечает на вопрос «Чья машина?»."
    },
    {
      "id": "tq_10",
      "type": "multipleChoice",
      "question": "Заполните пропуск: \"The teacher gave ___ homework.\" (нам)",
      "options": [
        "we",
        "our",
        "us",
        "ours"
      ],
      "correctIndex": 2,
      "explanation": "Us — object pronoun от we. Используется после глагола как получатель действия."
    }
