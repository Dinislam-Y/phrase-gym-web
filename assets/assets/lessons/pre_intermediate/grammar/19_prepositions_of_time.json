  "theory": {
    "category": "grammar",
    "title": "Prepositions of Time",
    "subtitle": "Три предлога — in, on, at — помогают сказать КОГДА что-то происходит. Каждый для своего: месяц, день или точное время.",
    "construction": {
      "formula": "IN (month/year/season) / ON (day/date) / AT (time)",
      "rules": [
        {
          "word": "in",
          "description": "Месяцы, годы, сезоны, части дня: in May, in 2024, in summer, in the morning."
        },
        {
          "word": "on",
          "description": "Дни недели и даты: on Monday, on March 5th, on my birthday."
        },
        {
          "word": "at",
          "description": "Точное время и моменты: at 5 o'clock, at noon, at night, at the weekend."
        }
      ]
    },
    "useCases": [
      {
        "icon": "schedule",
        "color": "blue",
        "title": "Расписание и время",
        "description": "Говорим, в какое точное время что-то происходит.",
        "example": {
          "english": "I wake up at seven.",
          "russian": "Я просыпаюсь в семь."
        }
      },
      {
        "icon": "star",
        "color": "purple",
        "title": "Дни и даты",
        "description": "Указываем конкретный день или дату.",
        "example": {
          "english": "We meet on Friday.",
          "russian": "Мы встречаемся в пятницу."
        }
      },
      {
        "icon": "explore",
        "color": "orange",
        "title": "Месяцы и сезоны",
        "description": "Говорим о периодах: месяцах, годах, сезонах.",
        "example": {
          "english": "It is warm in summer.",
          "russian": "Летом тепло."
        }
      }
    ],
    "markers": [
      "in January",
      "on Monday",
      "at 5 o'clock",
      "in the morning",
      "at night",
      "on my birthday"
    ]
  },
