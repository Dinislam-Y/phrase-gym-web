  "theory": {
    "category": "grammar",
    "title": "Basic Prepositions: In, On, At",
    "subtitle": "Предлоги in, on, at — три кита местоположения в английском. Каждый отвечает за свою зону: IN — внутри, ON — на поверхности, AT — в точке.",
    "construction": {
      "formula": "Subject + verb + in / on / at + place/time",
      "rules": [
        {
          "word": "IN (внутри)",
          "description": "Закрытые пространства (in the room), города и страны (in London), месяцы и годы (in May), части дня (in the morning / in the evening)."
        },
        {
          "word": "ON (на поверхности)",
          "description": "Поверхности (on the table, on the wall), дни недели (on Monday), даты (on May 5th), транспорт-платформы (on the bus)."
        },
        {
          "word": "AT (в точке)",
          "description": "Конкретные точки и места (at home, at school, at work), адреса (at 5 Baker Street), время (at night, at 5 o'clock)."
        }
      ]
    },
    "useCases": [
      {
        "icon": "explore",
        "color": "blue",
        "title": "IN — вы внутри",
        "description": "Представьте коробку: вы ВНУТРИ неё. Комната, город, страна — всё это контейнеры.",
        "example": {
          "english": "I am in the room.",
          "russian": "Я в комнате (внутри пространства)."
        }
      },
      {
        "icon": "visibility",
        "color": "green",
        "title": "ON — вы на поверхности",
        "description": "Представьте стол: книга лежит НА нём. Поверхность, день недели — это ON.",
        "example": {
          "english": "The book is on the table.",
          "russian": "Книга на столе (на поверхности)."
        }
      },
      {
        "icon": "star",
        "color": "purple",
        "title": "AT — вы в точке",
        "description": "Представьте точку на карте: вы именно ТАМ. Дом, школа, работа — конкретная точка.",
        "example": {
          "english": "She is at home.",
          "russian": "Она дома (в конкретной точке)."
        }
      },
      {
        "icon": "schedule",
        "color": "orange",
        "title": "Время: IN vs ON vs AT",
        "description": "IN the morning/evening, ON Monday/Friday, AT night/noon. Чем точнее время, тем меньше предлог: IN > ON > AT.",
        "example": {
          "english": "I work in the morning, on Monday, at nine.",
          "russian": "Я работаю утром, в понедельник, в девять."
        }
      }
    ],
    "markers": [
      "in the room / in London / in the morning",
      "on the table / on Monday / on the wall",
      "at home / at school / at night / at work",
      "in = внутри контейнера",
      "on = на поверхности",
      "at = в конкретной точке"
    ]
  },
