  "theory": {
    "category": "grammar",
    "title": "Plural Nouns",
    "subtitle": "Когда предметов больше одного, существительное меняется. Большинство просто получают -s, но есть хитрости.",
    "construction": {
      "formula": "Noun + -s / -es / -ies / irregular",
      "rules": [
        {
          "word": "+s",
          "description": "Большинство слов: cat → cats, book → books, dog → dogs."
        },
        {
          "word": "+es",
          "description": "После -s, -ss, -sh, -ch, -x, -o: box → boxes, dish → dishes, bus → buses."
        },
        {
          "word": "y → ies",
          "description": "Согласная + y: baby → babies, city → cities. НО: boy → boys (гласная + y)."
        },
        {
          "word": "Irregular",
          "description": "Слова-исключения: man → men, child → children, tooth → teeth, foot → feet, fish → fish."
        }
      ]
    },
    "useCases": [
      {
        "icon": "groups",
        "color": "blue",
        "title": "Когда больше одного",
        "description": "Считаем предметы, людей, животных.",
        "example": {
          "english": "I have two cats.",
          "russian": "У меня два кота."
        }
      },
      {
        "icon": "visibility",
        "color": "purple",
        "title": "Описание групп",
        "description": "Говорим о нескольких предметах сразу.",
        "example": {
          "english": "Many children play here.",
          "russian": "Много детей играют здесь."
        }
      },
      {
        "icon": "star",
        "color": "orange",
        "title": "Неправильные формы",
        "description": "Некоторые слова меняются полностью, их нужно запомнить.",
        "example": {
          "english": "Two men work here.",
          "russian": "Два мужчины работают здесь."
        }
      }
    ],
    "markers": [
      "two",
      "three",
      "many",
      "some",
      "a lot of",
      "several"
    ]
  },
