  "theory": {
    "category": "grammar",
    "title": "Prepositions of Place",
    "subtitle": "Предлоги места показывают, ГДЕ находится предмет. Самые важные: in (внутри), on (на поверхности), under (под).",
    "construction": {
      "formula": "S + be + preposition + noun",
      "rules": [
        {
          "word": "in",
          "description": "Внутри чего-то: in the box, in the bag, in the room."
        },
        {
          "word": "on",
          "description": "На поверхности: on the table, on the bed, on the chair."
        },
        {
          "word": "under",
          "description": "Под чем-то: under the table, under the bed."
        },
        {
          "word": "next to / behind / between",
          "description": "Рядом: next to the door. Позади: behind the chair. Между: between the boxes."
        }
      ]
    },
    "useCases": [
      {
        "icon": "explore",
        "color": "blue",
        "title": "Поиск вещей",
        "description": "Спрашиваем и говорим, где находятся предметы.",
        "example": {
          "english": "The book is on the table.",
          "russian": "Книга на столе."
        }
      },
      {
        "icon": "visibility",
        "color": "purple",
        "title": "Описание комнаты",
        "description": "Описываем расположение мебели и предметов.",
        "example": {
          "english": "The chair is next to the bed.",
          "russian": "Стул рядом с кроватью."
        }
      },
      {
        "icon": "favorite",
        "color": "orange",
        "title": "Животные и люди",
        "description": "Говорим, где кто сидит, стоит, лежит.",
        "example": {
          "english": "The cat is under the table.",
          "russian": "Кот под столом."
        }
      }
    ],
    "markers": [
      "in",
      "on",
      "under",
      "next to",
      "between",
      "behind"
    ]
  },
