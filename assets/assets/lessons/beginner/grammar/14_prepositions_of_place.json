  "theoryQuiz": [
    {
      "id": "tq_14_01",
      "type": "multipleChoice",
      "question": "Какой предлог означает «внутри»?",
      "options": [
        "on",
        "in",
        "under",
        "behind"
      ],
      "correctIndex": 1,
      "explanation": "In = внутри: in the box, in the bag, in the room."
    },
    {
      "id": "tq_14_02",
      "type": "multipleChoice",
      "question": "Выберите правильный вариант: «The book is ___ the table.» (Книга на столе)",
      "options": [
        "in",
        "under",
        "on",
        "behind"
      ],
      "correctIndex": 2,
      "explanation": "На поверхности = on: The book is on the table."
    },
    {
      "id": "tq_14_03",
      "type": "trueFalse",
      "question": "«The cat is under the bed» означает «Кот на кровати».",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 1,
      "explanation": "Under = под. «Кот под кроватью». На кровати = on the bed."
    },
    {
      "id": "tq_14_04",
      "type": "multipleChoice",
      "question": "Как сказать «рядом с»?",
      "options": [
        "behind",
        "under",
        "next to",
        "between"
      ],
      "correctIndex": 2,
      "explanation": "Next to = рядом с: The chair is next to the table."
    },
    {
      "id": "tq_14_05",
      "type": "multipleChoice",
      "question": "Какой предлог означает «между»?",
      "options": [
        "behind",
        "next to",
        "under",
        "between"
      ],
      "correctIndex": 3,
      "explanation": "Between = между: The cat is between the boxes."
    },
    {
      "id": "tq_14_06",
      "type": "trueFalse",
      "question": "«On» используется, когда предмет находится НА поверхности.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "On = на поверхности: on the table, on the bed, on the chair."
    },
    {
      "id": "tq_14_07",
      "type": "multipleChoice",
      "question": "Как правильно: «Собака за дверью»?",
      "options": [
        "The dog is in the door.",
        "The dog is on the door.",
        "The dog is behind the door.",
        "The dog is under the door."
      ],
      "correctIndex": 2,
      "explanation": "Behind = за, позади: The dog is behind the door."
    },
    {
      "id": "tq_14_08",
      "type": "multipleChoice",
      "question": "Выберите правильный предлог: «The ball is ___ the box.» (Мяч в коробке)",
      "options": [
        "on",
        "under",
        "behind",
        "in"
      ],
      "correctIndex": 3,
      "explanation": "In = внутри: The ball is in the box."
    },
    {
      "id": "tq_14_09",
      "type": "trueFalse",
      "question": "«Behind» означает «перед».",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 1,
      "explanation": "Behind = за, позади (не «перед»). Перед = in front of."
    },
