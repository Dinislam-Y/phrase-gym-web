  "theory": {
    "category": "grammar",
    "title": "Possessive Adjectives",
    "subtitle": "Притяжательные прилагательные показывают, КОМУ принадлежит вещь. Они всегда стоят ПЕРЕД существительным.",
    "construction": {
      "formula": "Possessive Adj + Noun",
      "rules": [
        {
          "word": "I → my",
          "description": "Я → мой: my book, my name, my cat."
        },
        {
          "word": "you → your",
          "description": "Ты/Вы → твой/ваш: your phone, your house."
        },
        {
          "word": "he → his, she → her",
          "description": "Он → его: his car. Она → её: her name."
        },
        {
          "word": "we → our, they → their",
          "description": "Мы → наш: our family. Они → их: their dog."
        }
      ]
    },
    "useCases": [
      {
        "icon": "handshake",
        "color": "blue",
        "title": "Представление",
        "description": "Знакомимся и говорим о своих вещах и людях.",
        "example": {
          "english": "My name is Tom.",
          "russian": "Меня зовут Том."
        }
      },
      {
        "icon": "groups",
        "color": "purple",
        "title": "Семья и друзья",
        "description": "Рассказываем о своей семье, друзьях, домашних животных.",
        "example": {
          "english": "Our dog is big.",
          "russian": "Наша собака большая."
        }
      },
      {
        "icon": "visibility",
        "color": "orange",
        "title": "Описание вещей",
        "description": "Указываем на принадлежность предметов.",
        "example": {
          "english": "This is her phone.",
          "russian": "Это её телефон."
        }
      }
    ],
    "markers": [
      "my",
      "your",
      "his",
      "her",
      "its",
      "our",
      "their"
    ]
  },
