  "theoryQuiz": [
    {
      "id": "tq_09_01",
      "type": "multipleChoice",
      "question": "Какое притяжательное прилагательное соответствует местоимению «she»?",
      "options": [
        "his",
        "her",
        "its",
        "their"
      ],
      "correctIndex": 1,
      "explanation": "She → her: Her name is Anna. Его (he) → his."
    },
    {
      "id": "tq_09_02",
      "type": "multipleChoice",
      "question": "Выберите правильный вариант: «___ car is red.» (Его машина красная.)",
      "options": [
        "He",
        "Him",
        "His",
        "Her"
      ],
      "correctIndex": 2,
      "explanation": "He → His: His car is red. Him — объектное местоимение, не притяжательное."
    },
    {
      "id": "tq_09_03",
      "type": "trueFalse",
      "question": "«This is mine book» — правильное предложение.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 1,
      "explanation": "Перед существительным используется my, а не mine: This is my book."
    },
    {
      "id": "tq_09_04",
      "type": "multipleChoice",
      "question": "Как правильно: «Наш дом большой»?",
      "options": [
        "Us house is big.",
        "We house is big.",
        "Our house is big.",
        "Ours house is big."
      ],
      "correctIndex": 2,
      "explanation": "We → Our: Our house is big. Us и ours не ставятся перед существительным."
    },
    {
      "id": "tq_09_05",
      "type": "multipleChoice",
      "question": "Какое притяжательное прилагательное у «they»?",
      "options": [
        "them",
        "they",
        "their",
        "theirs"
      ],
      "correctIndex": 2,
      "explanation": "They → their: Their dog is big. Them — объектное, theirs — абсолютное."
    },
    {
      "id": "tq_09_06",
      "type": "trueFalse",
      "question": "Притяжательные прилагательные всегда стоят ПЕРЕД существительным.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "Верно: my book, his car, our house — всегда перед существительным."
    },
    {
      "id": "tq_09_07",
      "type": "multipleChoice",
      "question": "Выберите правильный вариант: «___ name is Anna.» (Её зовут Анна.)",
      "options": [
        "She",
        "His",
        "Her",
        "Hers"
      ],
      "correctIndex": 2,
      "explanation": "She → Her: Her name is Anna."
    },
    {
      "id": "tq_09_08",
      "type": "multipleChoice",
      "question": "Как сказать «Моё имя — Том»?",
      "options": [
        "Me name is Tom.",
        "I name is Tom.",
        "My name is Tom.",
        "Mine name is Tom."
      ],
      "correctIndex": 2,
      "explanation": "I → my: My name is Tom. Me и mine не используются перед существительным."
    },
    {
      "id": "tq_09_09",
      "type": "trueFalse",
      "question": "«It's» и «its» — это одно и то же.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 1,
      "explanation": "It's = it is (сокращение). Its = его/её (притяжательное, без апострофа)."
    },
    {
      "id": "tq_09_10",
      "type": "multipleChoice",
      "question": "Выберите правильное предложение:",
      "options": [
        "This is you bag.",
        "This is your bag.",
        "This is yours bag.",
        "This is you're bag."
      ],
      "correctIndex": 1,
      "explanation": "You → your: This is your bag. You're = you are, yours используется без существительного."
