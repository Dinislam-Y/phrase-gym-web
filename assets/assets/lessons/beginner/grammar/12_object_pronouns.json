  "theory": {
    "category": "grammar",
    "title": "Object Pronouns",
    "subtitle": "Когда местоимение стоит ПОСЛЕ глагола (отвечает на «кого?» или «кому?»), мы используем объектную форму: me, him, her, us, them.",
    "construction": {
      "formula": "Verb + Object Pronoun",
      "rules": [
        {
          "word": "I → me",
          "description": "Я → мне/меня: Help me. Tell me."
        },
        {
          "word": "he → him, she → her",
          "description": "Он → его/ему: I know him. Она → её/ей: I see her."
        },
        {
          "word": "we → us, they → them",
          "description": "Мы → нас/нам: Help us. Они → их/им: I see them."
        },
        {
          "word": "you → you, it → it",
          "description": "Ты/Вы → тебя: I like you. Оно → его: I need it. Формы не меняются."
        }
      ]
    },
    "useCases": [
      {
        "icon": "waving_hand",
        "color": "blue",
        "title": "Просьбы",
        "description": "Просим помощи или информации.",
        "example": {
          "english": "Help me please.",
          "russian": "Помоги мне, пожалуйста."
        }
      },
      {
        "icon": "favorite",
        "color": "purple",
        "title": "Чувства к людям",
        "description": "Говорим, кто нам нравится или кого любим.",
        "example": {
          "english": "I love you.",
          "russian": "Я люблю тебя."
        }
      },
      {
        "icon": "visibility",
        "color": "orange",
        "title": "Описание действий",
        "description": "Говорим, кого видим, знаем, слышим.",
        "example": {
          "english": "I see them.",
          "russian": "Я вижу их."
        }
      }
    ],
    "markers": [
      "me",
      "you",
      "him",
      "her",
      "it",
      "us",
      "them"
    ]
  },
