  "theory": {
    "category": "grammar",
    "title": "Countable & Uncountable Nouns",
    "subtitle": "В английском есть вещи, которые можно посчитать (a book, two books), и которые нельзя (water, money). Это влияет на выбор слов.",
    "construction": {
      "formula": "a/an + countable | some + uncountable",
      "rules": [
        {
          "word": "Countable (исчисляемые)",
          "description": "Можно посчитать: a book, two chairs, three eggs. Используем a/an и many."
        },
        {
          "word": "Uncountable (неисчисляемые)",
          "description": "Нельзя посчитать: water, milk, money, bread. НЕТ артикля a/an, используем some и much."
        },
        {
          "word": "a / an",
          "description": "Только с исчисляемыми в единственном числе: a chair, an apple. НЕ a water!"
        },
        {
          "word": "some",
          "description": "С неисчисляемыми (some water) и множественным числом (some books)."
        }
      ]
    },
    "useCases": [
      {
        "icon": "favorite",
        "color": "blue",
        "title": "Еда и напитки",
        "description": "Большинство продуктов неисчисляемые: water, bread, rice, meat.",
        "example": {
          "english": "I need some water.",
          "russian": "Мне нужно воды."
        }
      },
      {
        "icon": "check_circle",
        "color": "purple",
        "title": "Предметы вокруг",
        "description": "Предметы можно посчитать: a chair, a cup, a book.",
        "example": {
          "english": "I have a chair.",
          "russian": "У меня есть стул."
        }
      },
      {
        "icon": "bolt",
        "color": "orange",
        "title": "Many vs Much",
        "description": "Many — для исчисляемых, much — для неисчисляемых.",
        "example": {
          "english": "I have many books.",
          "russian": "У меня много книг."
        }
      }
    ],
    "markers": [
      "a",
      "an",
      "some",
      "much",
      "many",
      "a lot of"
    ]
  },
