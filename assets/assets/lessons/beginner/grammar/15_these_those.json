  "theory": {
    "category": "Указательные местоимения",
    "title": "This / That / These / Those",
    "subtitle": "Эти рядом, а те далеко",
    "construction": {
      "formula": "This / That + is + noun  |  These / Those + are + noun(s)",
      "rules": [
        "This (этот/эта/это) — один предмет, рядом с говорящим.",
        "That (тот/та/то) — один предмет, далеко от говорящего.",
        "These (эти) — много предметов, рядом с говорящим.",
        "Those (те) — много предметов, далеко от говорящего.",
        "С this / that используем is: This is a pen. That is a car.",
        "С these / those используем are: These are pens. Those are cars.",
        "This / that + ед. число существительного: this book, that house.",
        "These / those + мн. число существительного: these books, those houses."
      ]
    },
    "useCases": [
      {
        "situation": "Показываешь что-то рядом",
        "example": "This is my phone.",
        "translation": "Это мой телефон."
      },
      {
        "situation": "Указываешь на что-то далеко",
        "example": "That is her house.",
        "translation": "То — её дом."
      },
      {
        "situation": "Несколько предметов рядом",
        "example": "These shoes are new.",
        "translation": "Эти туфли новые."
      },
      {
        "situation": "Несколько предметов далеко",
        "example": "Those cars are fast.",
        "translation": "Те машины быстрые."
      }
    ],
    "markers": [
      "this — ед. ч., рядом",
      "that — ед. ч., далеко",
      "these — мн. ч., рядом",
      "those — мн. ч., далеко",
      "here → this / these",
      "there → that / those"
    ]
  },
