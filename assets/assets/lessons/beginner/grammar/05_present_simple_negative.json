  "theoryQuiz": [
    {
      "id": "tq_01",
      "type": "trueFalse",
      "question": "Для отрицания в Present Simple с \"I\" используется \"doesn't\".",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 1,
      "explanation": "С I используется don't: I don't like coffee."
    },
    {
      "id": "tq_02",
      "type": "multipleChoice",
      "question": "Выберите правильный вариант: \"She ___ like spicy food.\"",
      "options": [
        "don't",
        "doesn't",
        "isn't",
        "not"
      ],
      "correctIndex": 1,
      "explanation": "С she используется doesn't для отрицания в Present Simple."
    },
    {
      "id": "tq_03",
      "type": "multipleChoice",
      "question": "Какая формула отрицания в Present Simple?",
      "options": [
        "Subject + not + V1",
        "Subject + don't/doesn't + V1",
        "Subject + V1 + not",
        "Subject + isn't + V1"
      ],
      "correctIndex": 1,
      "explanation": "Отрицание строится по формуле: Subject + don't/doesn't + V1 (глагол без -s)."
    },
    {
      "id": "tq_04",
      "type": "trueFalse",
      "question": "После \"doesn't\" глагол стоит без окончания -s: He doesn't play.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "После doesn't глагол всегда используется в базовой форме без -s."
    },
    {
      "id": "tq_05",
      "type": "multipleChoice",
      "question": "Найдите ошибку: \"He doesn't goes to the gym.\"",
      "options": [
        "Нужно \"don't\" вместо \"doesn't\"",
        "Нужно \"go\" вместо \"goes\"",
        "Нужно \"going\" вместо \"goes\"",
        "Ошибки нет"
      ],
      "correctIndex": 1,
      "explanation": "После doesn't глагол теряет -s/-es: He doesn't go to the gym."
    },
    {
      "id": "tq_06",
      "type": "trueFalse",
      "question": "\"We doesn't want pizza.\" — предложение составлено правильно.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 1,
      "explanation": "С we используется don't: We don't want pizza."
    },
    {
      "id": "tq_07",
      "type": "multipleChoice",
      "question": "С какими местоимениями используется \"don't\"?",
      "options": [
        "he, she, it",
        "I, you, we, they",
        "только I и you",
        "только we и they"
      ],
      "correctIndex": 1,
      "explanation": "Don't используется с I, you, we, they. Doesn't — с he, she, it."
    },
    {
      "id": "tq_08",
      "type": "multipleChoice",
      "question": "Выберите правильное предложение:",
      "options": [
        "They doesn't eat meat.",
        "She don't speak French.",
        "I don't have a car.",
        "He don't know the answer."
      ],
      "correctIndex": 2,
      "explanation": "I + don't — верная комбинация. They don't, She doesn't, He doesn't — в остальных ошибки."
    },
    {
      "id": "tq_09",
      "type": "trueFalse",
      "question": "\"Don't\" — это сокращённая форма от \"do not\".",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "Don't = do not, doesn't = does not."
    },
    {
      "id": "tq_10",
      "type": "multipleChoice",
      "question": "Как правильно перевести: «Она не работает по воскресеньям»?",
      "options": [
        "She don't work on Sundays.",
        "She doesn't works on Sundays.",
        "She doesn't work on Sundays.",
        "She not work on Sundays."
      ],
      "correctIndex": 2,
      "explanation": "She + doesn't + work (без -s). Это правильная формула отрицания в Present Simple."
    }
