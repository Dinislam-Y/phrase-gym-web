  "theory": {
    "category": "grammar",
    "title": "Prepositions",
    "subtitle": "Представьте пирамиду. Сверху (острая точка) — AT (Точное место/время). В середине (плоскость) — ON (Улица/День). Внизу (объемное пространство) — IN (Город/Месяц).",
    "construction": {
      "formula": "Preposition + Noun",
      "rules": [
        {
          "word": "AT (Точка)",
          "description": "Точное время (at 5 PM). Точное место (at home, at the door). Конкретная точка на карте или часах."
        },
        {
          "word": "ON (Поверхность/День)",
          "description": "Дни недели (on Monday). Даты (on May 1st). Поверхности (on the table, on the floor)."
        },
        {
          "word": "IN (Объем)",
          "description": "Долгие периоды (in 2024, in Summer). Пространства (in the room, in London, in a box)."
        },
        {
          "word": "Направление",
          "description": "TO (к цели), INTО (внутрь), OUT OF (наружу)."
        }
      ]
    },
    "useCases": [
      {
        "icon": "explore",
        "color": "blue",
        "title": "Точка или Объем?",
        "description": "I am AT the cinema (как точка на карте). I am IN the cinema (внутри здания).",
        "example": {
          "english": "Meet me at the station.",
          "russian": "Встретимся на станции (точка встречи)."
        }
      },
      {
        "icon": "schedule",
        "color": "green",
        "title": "Время",
        "description": "At 5:00. On Friday. In July.",
        "example": {
          "english": "See you on Monday.",
          "russian": "Увидимся в понедельник."
        }
      },
      {
        "icon": "bolt",
        "color": "purple",
        "title": "Движение",
        "description": "Go TO school. Walk INTO the room.",
        "example": {
          "english": "He jumped into the water.",
          "russian": "Он прыгнул в (внутрь) воду."
        }
      }
    ],
    "markers": [
      "in",
      "on",
      "at",
      "to",
      "into",
      "from",
      "under",
      "behind",
      "between"
    ]
  },
