  "theoryQuiz": [
    {
      "id": "tq_01",
      "type": "trueFalse",
      "question": "Present Perfect образуется: have/has + V3 (Past Participle).",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "Формула Present Perfect: have/has + третья форма глагола."
    },
    {
      "id": "tq_02",
      "type": "multipleChoice",
      "question": "Когда используется 'has' в Present Perfect?",
      "options": [
        "с I, you, we, they",
        "с he, she, it",
        "со всеми местоимениями",
        "только с I"
      ],
      "correctIndex": 1,
      "explanation": "Has используется с третьим лицом единственного числа: he, she, it."
    },
    {
      "id": "tq_03",
      "type": "multipleChoice",
      "question": "Какое слово является маркером Present Perfect?",
      "options": [
        "yesterday",
        "last week",
        "already",
        "ago"
      ],
      "correctIndex": 2,
      "explanation": "Already — типичный маркер Present Perfect. Yesterday и ago — маркеры Past Simple."
    },
    {
      "id": "tq_04",
      "type": "trueFalse",
      "question": "Можно сказать: 'I have finished yesterday.'",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 1,
      "explanation": "Present Perfect нельзя использовать с точным временем в прошлом (yesterday). Правильно: I finished yesterday."
    },
    {
      "id": "tq_05",
      "type": "multipleChoice",
      "question": "Выберите правильный вариант: She ___ to Paris.",
      "options": [
        "has go",
        "have gone",
        "has gone",
        "has went"
      ],
      "correctIndex": 2,
      "explanation": "She + has + V3: She has gone (go → went → gone)."
    },
    {
      "id": "tq_06",
      "type": "trueFalse",
      "question": "Маркер 'never' используется с Present Perfect.",
      "options": [
        "Верно",
        "Неверно"
      ],
      "correctIndex": 0,
      "explanation": "Never — маркер Present Perfect: I have never been to Japan."
    },
    {
      "id": "tq_07",
      "type": "multipleChoice",
      "question": "Какая разница между 'since' и 'for'?",
      "options": [
        "since — период, for — точка",
        "since — точка начала, for — период",
        "нет разницы",
        "since — будущее, for — прошлое"
      ],
      "correctIndex": 1,
      "explanation": "Since указывает на точку начала (since 2020), for — на период (for 3 years)."
    },
    {
      "id": "tq_08",
      "type": "multipleChoice",
      "question": "Какое предложение построено правильно?",
      "options": [
        "I have saw this film.",
        "She has already eaten.",
        "They has finished.",
        "He have gone home."
      ],
      "correctIndex": 1,
      "explanation": "She + has + V3 (eaten) + маркер already — верная конструкция."
    },
    {
      "id": "tq_09",
      "type": "trueFalse",
      "question": "В Present Perfect важен результат действия в настоящем, а не время его совершения.",
