  "theory": {
    "category": "grammar",
    "title": "Past Continuous",
    "subtitle": "Это «Видеозапись» прошлого, а не «Фотография». Мы видим процесс, который длился в определенный момент. Часто используется как фон для более коротких событий (Past Simple).",
    "construction": {
      "formula": "Was/Were + V-ing",
      "rules": [
        {
          "word": "I / He / She / It",
          "description": "Was + глагол с окончанием -ing. I was sleeping. It was raining."
        },
        {
          "word": "You / We / They",
          "description": "Were + глагол с окончанием -ing. They were playing. We were cooking."
        },
        {
          "word": "Прерванное действие",
          "description": "Длительное действие (Continuous) прерывается коротким (Simple). I was reading when he came."
        },
        {
          "word": "Два процесса",
          "description": "Два действия происходили одновременно. While I was reading, she was cooking."
        }
      ]
    },
    "useCases": [
      {
        "icon": "visibility",
        "color": "blue",
        "title": "Действие в определенный момент",
        "description": "Мы не знаем, когда началось и закончилось, мы внутри процесса.",
        "example": {
          "english": "At 8 pm I was watching TV.",
          "russian": "В 8 вечера я смотрел телевизор."
        }
      },
      {
        "icon": "bolt",
        "color": "orange",
        "title": "Прерванное действие",
        "description": "Фон (Continuous) и вспышка (Simple).",
        "example": {
          "english": "I was walking when I met her.",
          "russian": "Я шел (процесс), когда встретил ее (момент)."
        }
      },
      {
        "icon": "visibility",
        "color": "blue",
        "title": "Атмосфера и описание",
        "description": "Описание обстановки в историях.",
        "example": {
          "english": "The sun was shining, birds were singing.",
          "russian": "Солнце светило, птицы пели."
        }
      }
    ],
    "markers": [
      "at 5 o'clock",
      "when",
      "while",
      "all day",
      "the whole evening"
    ]
  },
